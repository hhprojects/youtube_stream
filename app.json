{
  "expo": {
    "name": "YouTube Streamer",
    "slug": "youtube-streamer",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "youtube-streamer",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.youtubestreamer.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.youtubestreamer.app"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
