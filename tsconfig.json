{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es2017", "es6", "es5"],
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
